{
  "coin": {
      "name": "Kumacoin",
      "shortcut": "KUMA",
      "label": "Kumacoin",
      "alias": "kumacoin"
  },
  "ports": {
    "backend_rpc": 8099,
    "backend_message_queue": 38399,
    "blockbook_internal": 9099,
    "blockbook_public": 9199
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-kumacoin",
    "package_revision": "satoshilabs-1",
    "system_user": "kumacoin",
    "version": "20190326",
    "binary_url": "https://github.com/wakiyamap/kumacoin/releases/download/20190326/kumacoin-20190326-x86_64-linux-gnu.tar.gz",
    "verification_type": "sha256",
    "verification_source": "1d3244d8d497c9328d07b64c920c9de3531645304de6a6e67fcb764cd6ac4b0f",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/kumacoind -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
        "whitelist": "127.0.0.1"
    }
  },
  "blockbook": {
    "package_name": "blockbook-kumacoin",
    "system_user": "blockbook-kumacoin",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "-resyncindexperiod=100011 -resyncmempoolperiod=10011",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 10000,
      "xpub_magic": 76067358,
      "xpub_magic_segwit_p2sh": 77429938,
      "xpub_magic_segwit_native": 78792518,
      "slip44": 22,
      "additional_params": {
        "fiat_rates": "coingecko",
        "fiat_rates_params": "{\"url\": \"https://api.coingecko.com/api/v3\", \"coin\": \"kumacoin\", \"periodSeconds\": 60}"
      }
    }
  },
  "meta": {
    "package_maintainer": "Jakub Matys",
    "package_maintainer_email": "jakub.matys@satoshilabs.com"
  }
}
